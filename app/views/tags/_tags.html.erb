<div class="row cust-con" %>
  <div class="col-xs-offset-1 col-xs-10" >
    <div class="row">
      <% tags.uniq.each do |tag| %>
        <div class="col-xs-6 col-sm-3">
        <a href="/tag/<%= tag.id %>"><button class="btn-default"><%= tag.name %></button></a>
        </div>
      <% end %>
    </div>
  </div>
</div>
